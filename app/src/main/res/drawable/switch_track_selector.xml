<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <!--<padding android:bottom="@dimen/dp_10" android:left="@dimen/dp_10" android:right="@dimen/dp_10" android:top="@dimen/dp_10" />-->
            <stroke android:width="1dp" android:color="#A0A0A0" />
            <solid android:color="#EDEDED" />
            <corners android:radius="100dp" />
        </shape>
    </item>

    <item android:state_checked="true">

        <shape android:shape="rectangle">
            <!--<padding android:bottom="@dimen/dp_10" android:left="@dimen/dp_10" android:right="@dimen/dp_10" android:top="@dimen/dp_10" />-->
            <stroke android:width="1dp" android:color="#009405" />
            <solid android:color="#EAFBEC" />
            <corners android:radius="100dp" />
        </shape>
    </item>

</selector>