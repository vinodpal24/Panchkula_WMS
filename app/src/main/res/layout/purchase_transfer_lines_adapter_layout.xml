                <TableRow
                    android:id="@+id/scan_View"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="3dp"
                    android:visibility="visible">

                    <TextView
                        android:id="@+id/qr_title"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="0.5"
                        android:fontFamily="@font/muli_bold"
                        android:text="Batch Code :  "
                        android:textSize="12sp" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="4dp"
                        android:layout_weight="1.5"
                        android:background="@drawable/round_gray_backgruand"
                        android:fontFamily="@font/muli_bold"
                        android:gravity="center_vertical"
                        android:hint="Scan Item"
                        android:orientation="horizontal"
                        android:paddingTop="5sp"
                        android:paddingBottom="5sp"
                        app:boxBackgroundColor="@android:color/transparent"
                        app:boxStrokeColor="@color/colorPrimary">

                        <EditText
                            android:id="@+id/ed_batch_code_scan"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@null"
                            android:backgroundTint="@color/my_app_background_color"
                            android:fontFamily="@font/muli_bold"
                            android:hint="Scan item"
                            android:paddingLeft="8sp"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="12sp" />

                        <!--                            android:imeOptions="actionDone|actionGo|actionSend"
-->

                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/iv_scan_batch_code"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginRight="8sp"
                            android:src="@drawable/ic_baseline_qr_code_scanner_24" />

                    </LinearLayout>
                </TableRow>

                <TableRow
                    android:id="@+id/ven_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="5dp"
                    android:paddingTop="3dp"
                    android:paddingRight="5dp"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="0.5"
                        android:fontFamily="@font/muli_bold"
                        android:text="Vender Ref No :  "
                        android:textSize="12sp" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="4dp"
                        android:layout_weight="1.5"
                        android:background="@drawable/round_gray_backgruand"
                        android:fontFamily="@font/muli_bold"
                        android:gravity="center_vertical"
                        android:hint="Scan Item"
                        android:orientation="horizontal"
                        android:paddingTop="5sp"
                        android:paddingBottom="5sp"
                        app:boxBackgroundColor="@android:color/transparent"
                        app:boxStrokeColor="@color/colorPrimary">

                        <EditText
                            android:id="@+id/ed_ved_no"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@null"
                            android:backgroundTint="@color/my_app_background_color"
                            android:fontFamily="@font/muli_bold"
                            android:hint="Vender Ref. No"
                            android:paddingLeft="8sp"
                            android:singleLine="true"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="12sp" />

                        <!--                            android:imeOptions="actionDone|actionGo|actionSend"
-->


                    </LinearLayout>
                </TableRow>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingTop="3dp">

                    <LinearLayout
                        android:id="@+id/scaned_item_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4dp"
                        android:background="@drawable/round_gray_backgruand"
                        android:fontFamily="@font/muli_bold"
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:padding="8dp"
                        android:visibility="gone">

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingTop="3dp"
                            android:paddingBottom="3dp">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_weight="0.8"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/muli_bold"
                                    android:text="Batch        :  "
                                    android:textColor="@color/black"
                                    android:textSize="12sp" />

                                <TextView
                                    android:id="@+id/tv_batch"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_weight="0.5"
                                    android:fontFamily="@font/muli_bold"
                                    android:text="1000233"
                                    android:textSize="12sp" />
                            </LinearLayout>


                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/muli_bold"
                                    android:text="Doc Entry            :  "
                                    android:textColor="@color/black"
                                    android:textSize="12sp" />

                                <TextView
                                    android:id="@+id/tv_doc_entry"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_weight="0.5"
                                    android:fontFamily="@font/muli_bold"
                                    android:text="102"
                                    android:textSize="12sp" />
                            </LinearLayout>


                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingTop="3dp"
                            android:paddingBottom="3dp">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_weight="0.8"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/muli_bold"
                                    android:text="ItemCode  :  "
                                    android:textColor="@color/black"
                                    android:textSize="12sp" />

                                <TextView
                                    android:id="@+id/tv_item_code"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_weight="3"
                                    android:fontFamily="@font/muli_bold"
                                    android:text="FL-JUMBO"
                                    android:textSize="12sp" />
                            </LinearLayout>


                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/muli_bold"
                                    android:text="ItemDescription  :  "
                                    android:textColor="@color/black"
                                    android:textSize="12sp" />

                                <TextView
                                    android:id="@+id/tv_item_desc"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_weight="3"
                                    android:fontFamily="@font/muli_bold"
                                    android:text="FL-JUMBO"
                                    android:textSize="12sp" />
                            </LinearLayout>


                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingTop="3dp"
                            android:paddingBottom="3dp">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="5dp"
                                android:layout_weight="1"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/muli_bold"
                                    android:text="Status       :  "
                                    android:textColor="@color/black"
                                    android:textSize="12sp" />

                                <TextView
                                    android:id="@+id/tv_status"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_weight="3"
                                    android:fontFamily="@font/muli_bold"
                                    android:text="bdsStatus_Released"
                                    android:textSize="12sp" />
                            </LinearLayout>

                        </TableRow>

                    </LinearLayout>

                    <androidx.recyclerview.widget.RecyclerView
