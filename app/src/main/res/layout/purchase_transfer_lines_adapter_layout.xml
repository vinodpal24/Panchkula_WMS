<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_card"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="@dimen/size_8"
    android:layout_marginBottom="@dimen/size_8">

        <LinearLayout
            android:id="@+id/mainLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@drawable/bg_docline_item"
            android:paddingHorizontal="@dimen/size_10"
            android:paddingTop="@dimen/size_10"
            android:paddingBottom="@dimen/size_5">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="visible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent">

                    <LinearLayout
                        android:id="@+id/layoutItemCode"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintWidth_percent="0.40"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/layoutTotalScanQty"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:id="@+id/tvItemCodeLabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:includeFontPadding="false"
                            android:text="Item Code"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_12"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvItemCode"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:includeFontPadding="false"
                            android:paddingEnd="@dimen/size_8"
                            android:textStyle="bold"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_12" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/layoutTotalScanQty"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintWidth_percent="0.30"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/layoutOpenQty"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/layoutItemCode"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:id="@+id/tvTotalScanQtyLabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:includeFontPadding="false"
                            android:text="Total Scan Qty."
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_12"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvTotalScannQty"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:includeFontPadding="false"
                            android:paddingEnd="@dimen/size_8"
                            android:text="0.00"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_12" />


                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/layoutOpenQty"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintWidth_percent="0.30"
                        android:orientation="vertical"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/layoutTotalScanQty"
                        app:layout_constraintTop_toTopOf="@+id/layoutTotalScanQty">

                        <TextView
                            android:id="@+id/tvOpenQtyLabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:includeFontPadding="false"
                            android:text="Open Qty."
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_12"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvOpenQty"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:includeFontPadding="false"
                            android:paddingEnd="@dimen/size_8"
                            android:text="0.00"
                            android:textAllCaps="true"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_12" />


                    </LinearLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/tr_total_scan_fields"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_8"
                    android:visibility="visible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent">
                    <LinearLayout
                        android:id="@+id/layoutItemName"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintWidth_percent="0.70"
                        android:orientation="vertical"
                        app:layout_constraintEnd_toStartOf="@+id/layoutItemType"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <TextView
                            android:id="@+id/tvItemNameLabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:includeFontPadding="false"
                            android:text="Item Name"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_12"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvItemName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:includeFontPadding="false"
                            android:paddingEnd="@dimen/size_8"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_12" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/layoutItemType"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        app:layout_constraintWidth_percent="0.30"
                        android:orientation="vertical"
                       app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/layoutItemName"
                        app:layout_constraintStart_toEndOf="@+id/layoutItemName"
                        >

                        <TextView
                            android:id="@+id/tvItemTypeLabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:includeFontPadding="false"
                            android:text="ItemType"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_12"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvTotalScanGw"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:includeFontPadding="false"
                            android:paddingEnd="@dimen/size_8"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_12" />


                    </LinearLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>
                <LinearLayout
                    android:id="@+id/layoutTax"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/size_8"
                    app:layout_constraintWidth_percent="0.30"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/layoutItemName"
                    app:layout_constraintStart_toEndOf="@+id/layoutItemName"
                    >
                    <TextView
                        android:id="@+id/tvTaxLabel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:includeFontPadding="false"
                        android:text="Tax"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_12"
                        android:textStyle="bold" />
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/layoutTaxPercent"
                        style="@style/OutlinedTextInputLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintWidth_percent="0.30"
                        android:layout_marginTop="0dp"
                        android:layout_marginEnd="@dimen/size_4"
                        android:layout_weight="0.65"
                        android:hint="Select Tax"
                        app:endIconMode="dropdown_menu"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/layoutItemType"
                        app:layout_constraintStart_toEndOf="@+id/layoutItemType">

                        <AutoCompleteTextView
                            android:id="@+id/acTaxCode"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:backgroundTint="@color/white"
                            android:completionThreshold="1"
                            android:focusable="false"
                            android:inputType="text"
                            android:paddingStart="@dimen/size_15"
                            android:textSize="@dimen/text_size_11" />
                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>


